{
  "name": "shadcn-ui-monorepo",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "format": "npx ultracite@latest format",
    "lint": "npx ultracite@latest lint",
    "web:dev": "turbo dev --filter=web",
    "web:build": "turbo build --filter=web",
    "web:start": "pnpm --filter web start",
    "domain:build": "pnpm --filter @workspace/domain-ui registry:build",
    "domain-pro:build": "pnpm --filter @workspace/domain-ui-pro registry:build"
  },
  "devDependencies": {
    "@biomejs/biome": "2.1.2",
    "@eslint/js": "^9.32.0",
    "@workspace/eslint-config": "workspace:*",
    "@workspace/typescript-config": "workspace:*",
    "husky": "^9.1.7",
    "turbo": "^2.4.2",
    "typescript": "5.7.3",
    "ultracite": "5.1.2"
  },
  "packageManager": "pnpm@10.4.1",
  "engines": {
    "node": ">=20"
  }
}
